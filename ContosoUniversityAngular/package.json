{
  "name": "contoso-university-angular",
  "description": "Microsoft's Contoso University ASP.Net Core tutorial converted to use Angular 4.x for the UI part",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --verbose --progress --vendor-chunk --extract-css",
    "start-prod": "ng serve --open --verbose --progress --vendor-chunk --extract-css --prod",
    "build": "ng build --progress --verbose --vendor-chunk --extract-css",
    "build-watch": "ng build --progress --verbose --vendor-chunk --extract-css --watch",
    "build-prod": "ng build --progress --verbose --vendor-chunk --extract-css --prod",
    "build-prod-watch": "ng build --progress --verbose --vendor-chunk --extract-css --prod --watch",
    "test": "ng test",
    "lint": "ng lint --typeCheck --format stylish",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
