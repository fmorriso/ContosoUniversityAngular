    "karma-jasmine": "^1.0.0",
    "karma-jasmine-html-reporter": "^0.2.0",
    "protractor": "^5.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0"
