    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.1",
    "@progress/kendo-angular-buttons": "^1.0.0",
    "@progress/kendo-angular-dateinputs": "^1.0.0",
    "@progress/kendo-angular-dropdowns": "^1.0.0",
    "@progress/kendo-angular-excel-export": "^1.0.0",
    "@progress/kendo-angular-grid": "^1.0.0",
